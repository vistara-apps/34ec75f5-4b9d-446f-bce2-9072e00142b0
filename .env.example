# OnchainKit API Key (required for Base integration)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# Base RPC URL (optional - defaults to public RPC)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Chainlink VRF Configuration (for production)
CHAINLINK_VRF_COORDINATOR=0x...
CHAINLINK_VRF_KEY_HASH=0x...
CHAINLINK_VRF_SUBSCRIPTION_ID=123

# Database URL (for production)
DATABASE_URL=postgresql://...

# Webhook Secret (for frame updates)
WEBHOOK_SECRET=your_webhook_secret_here
